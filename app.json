{
  "expo": {
    "name": "NicMeUp",
    "slug": "NicMeUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/Logo4.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.conor.nicmeup",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification", "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UILaunchStoryboardName": "LaunchScreen",
        "NSLocationWhenInUseUsageDescription": "Allow Nic-Me-Up to access your location for showing nearby NicAssists",
        "NSLocationAlwaysUsageDescription": "Allow Nic-Me-Up to access your location even when the app is in the background"
      }
    },
    "plugins": [
      "expo-web-browser",
      "expo-notifications",
      [
        "expo-notifications",
        {
          "icon": "./assets/Logo4.png",
          "mode": "production",
          "displayInForeground": true,
          "remoteNotification": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "54b5732a-090a-4f7d-88fd-46fd3fd1e649"
      }
    },
    "owner": "cwan7",
    "android": {
      "package": "com.conor.nicmeup"
    }
  }
}
